{
    "version": 2,
    "buildCommand": "npm run vercel-build",
    "outputDirectory": "public",
    "framework": null,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.6.0"
        }
    },
    "routes": [
        { "src": "/build/(.*)", "dest": "/build/$1" },
        { "src": "/(.*)", "dest": "/api/index.php" }
    ]
} 